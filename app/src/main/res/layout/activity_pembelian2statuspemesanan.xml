<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/kotaknol"
    android:paddingLeft="@dimen/kotaknol"
    android:paddingRight="@dimen/kotaknol"
    android:paddingTop="@dimen/kotaknol"
    android:background="#8e292d"
    tools:context="com.example.muffinman.myapplication.utama">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/kotaklimalima"
        android:paddingLeft="@dimen/kotaknol"
        android:paddingRight="@dimen/kotaknol"
        android:paddingTop="50dp"
        android:background="#8e292d"
        tools:context="com.example.muffinman.myapplication.utama">

        <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="@dimen/kotaknol"
            android:paddingLeft="@dimen/kotaknol"
            android:paddingRight="@dimen/kotaknol"
            android:paddingTop="@dimen/kotaknol"
            android:background="#f1e5d9"
            tools:context="com.example.muffinman.myapplication.utama">

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/scrollView3">
                <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                    xmlns:tools="http://schemas.android.com/tools"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingBottom="@dimen/kotak16"
                    android:paddingLeft="@dimen/kotak16"
                    android:paddingRight="@dimen/kotak16"
                    android:paddingTop="@dimen/kotak16"
                    tools:context="com.example.muffinman.myapplication.utama">

                <RelativeLayout
                    android:layout_width="360dp"
                    android:layout_height="150dp"
                    android:layout_centerHorizontal="true"
                    android:id="@+id/relative1id"
                   >

                <ImageView
                    android:layout_width="360dp"
                    android:layout_height="150dp"
                    android:src="@drawable/kotaktransaksi"
                    android:layout_centerHorizontal="true"
                    android:id="@+id/kotaktransaksi"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="No. Transaksi"
                        android:id="@+id/notransaksi"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginLeft="9dp"
                        android:layout_marginStart="9dp"
                        android:layout_marginTop="20dp"
                        android:textColor="#FFF" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="15-Feb-2016"
                        android:id="@+id/tanggaltransaksi"
                        android:layout_alignTop="@+id/notransaksi"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginRight="20dp"
                        android:layout_marginEnd="20dp"
                        android:textColor="#FFF" />

                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/gambariconpenjualan"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginLeft="20dp"
                        android:layout_marginBottom="25dp"
                        android:id="@+id/gambarproduk"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Nama Barang"
                        android:id="@+id/namabarang"
                        android:textColor="#FFF"
                        android:layout_alignTop="@+id/gambarproduk"
                        android:layout_toRightOf="@+id/gambarproduk"
                        android:layout_toEndOf="@+id/gambarproduk"
                        android:layout_marginLeft="15dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Nama Penjual"
                        android:id="@+id/namapenjual"
                        android:textColor="#FFF"
                       android:textSize="13dp"
                        android:layout_toRightOf="@+id/gambarproduk"
                        android:layout_toEndOf="@+id/gambarproduk"
                        android:layout_marginLeft="15dp"
                        android:layout_below="@id/namabarang"/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="360dp"
                    android:layout_height="150dp"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@id/relative1id"
                    android:layout_marginTop="10dp"
                    android:id="@+id/relative1id2"
                    >

                    <ImageView
                        android:layout_width="360dp"
                        android:layout_height="150dp"
                        android:src="@drawable/kotaktransaksi"
                        android:layout_centerHorizontal="true"
                        android:id="@+id/kotaktransaksi2"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="No. Transaksi"
                        android:id="@+id/notransaksi2"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginLeft="9dp"
                        android:layout_marginStart="9dp"
                        android:layout_marginTop="20dp"
                        android:textColor="#FFF" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="15-Feb-2016"
                        android:id="@+id/tanggaltransaksi2"
                        android:layout_alignTop="@+id/notransaksi2"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginRight="20dp"
                        android:layout_marginEnd="20dp"
                        android:textColor="#FFF" />

                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/gambariconpenjualan"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginLeft="20dp"
                        android:layout_marginBottom="25dp"
                        android:id="@+id/gambarproduk2"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Nama Barang"
                        android:id="@+id/namabarang2"
                        android:textColor="#FFF"
                        android:layout_alignTop="@+id/gambarproduk2"
                        android:layout_toRightOf="@+id/gambarproduk2"
                        android:layout_toEndOf="@+id/gambarproduk2"
                        android:layout_marginLeft="15dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Nama Penjual"
                        android:id="@+id/namapenjual2"
                        android:textColor="#FFF"
                        android:textSize="13dp"
                        android:layout_toRightOf="@+id/gambarproduk2"
                        android:layout_toEndOf="@+id/gambarproduk2"
                        android:layout_marginLeft="15dp"
                        android:layout_below="@id/namabarang2"/>

                </RelativeLayout>
            </RelativeLayout>
        </ScrollView>
            </RelativeLayout>

        </RelativeLayout>

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/pengaturan"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:src="@drawable/logostatuspemesanan"

        android:layout_centerHorizontal="true"
        />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:src="@drawable/beranda"
        android:id="@+id/berandatombol" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/akunsaya"
        android:id="@+id/akuntombol"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/pencarian"
        android:layout_alignTop="@+id/favtombol"
        android:layout_centerInParent="true"
        android:id="@+id/pencariantombol" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/favorite"
        android:id="@+id/favtombol"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/berandatombol"
        android:layout_toEndOf="@+id/berandatombol"
        android:layout_toStartOf="@id/pencariantombol"
        android:layout_toLeftOf="@id/pencariantombol"/>
    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/berita"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@id/pencariantombol"
        android:layout_toEndOf="@id/pencariantombol"
        android:layout_toStartOf="@id/akuntombol"
        android:layout_toLeftOf="@id/akuntombol" />
</RelativeLayout>
