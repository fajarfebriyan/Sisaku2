<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/atasbawah"
    android:paddingLeft="@dimen/kanankiri"
    android:paddingRight="@dimen/kanankiri"
    android:paddingTop="@dimen/atasbawah"
    android:background="#8e292d"
    tools:context="com.example.muffinman.myapplication.utama">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="55dp"
        android:paddingLeft="@dimen/kanankiri"
        android:paddingRight="@dimen/kanankiri"
        android:paddingTop="@dimen/atasbawah2"
        android:background="#8e292d"
        tools:context="com.example.muffinman.myapplication.utama">

        <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="@dimen/activity_vertical_margin"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:paddingTop="@dimen/activity_vertical_margin"
            android:background="#f1e5d9"
            tools:context="com.example.muffinman.myapplication.utama">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="@dimen/atasbawah"
            android:paddingLeft="@dimen/kanankiri"
            android:paddingRight="@dimen/kanankiri"
            android:paddingTop="@dimen/atasbawah"

            tools:context="com.example.muffinman.myapplication.utama">

                <ImageView
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:src="@drawable/rumah"
                    android:id="@+id/rumahtombol"
                    android:layout_alignParentLeft="true"/>
                <ImageView
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:src="@drawable/hadiah"
                    android:id="@+id/hadiahtombol"
                    android:layout_centerHorizontal="true"/>
                <ImageView
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:src="@drawable/bunga"
                    android:id="@+id/bungatombol"
                    android:layout_alignParentRight="true"/>
                <ImageView
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:src="@drawable/office"
                    android:layout_marginTop="60dp"
                    android:layout_below="@+id/rumahtombol"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:id="@+id/officetombol" />
                <ImageView
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:src="@drawable/fashion"
                    android:id="@+id/fashiontombol"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@id/hadiahtombol"
                    android:layout_marginTop="60dp"/>
                <ImageView
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:src="@drawable/bola"
                    android:id="@+id/bolatombol"
                    android:layout_below="@+id/bungatombol"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="60dp"/>
                <ImageView
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:src="@drawable/dapur"
                    android:id="@+id/dapurtombol"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/officetombol"
                    android:layout_marginTop="60dp"/>
                <ImageView
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:src="@drawable/other"
                    android:id="@+id/othertombol"
                    android:layout_below="@id/fashiontombol"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="60dp"/>
                <TextView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:text="Rumah Tangga"
                    android:layout_alignRight="@+id/rumahtombol"
                    android:layout_alignLeft="@id/rumahtombol"
                    android:layout_alignEnd="@+id/rumahtombol"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:gravity="center"
                    android:layout_below="@+id/rumahtombol"
                    />
                <TextView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:text="Hadiah Souvenir"
                    android:layout_below="@id/hadiahtombol"
                    android:layout_alignLeft="@+id/hadiahtombol"
                    android:layout_alignStart="@+id/hadiahtombol"
                    android:layout_alignRight="@+id/hadiahtombol"
                    android:layout_alignEnd="@+id/hadiahtombol"
                    android:gravity="center"
                    android:id="@+id/textView2" />
                <TextView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:text="Hiasan"
                    android:layout_below="@+id/bungatombol"
                    android:layout_alignLeft="@+id/bungatombol"
                    android:layout_alignStart="@+id/bungatombol"
                    android:layout_alignRight="@+id/bungatombol"
                    android:layout_alignEnd="@+id/bungatombol"
                    android:gravity="center" />

            <TextView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:text="Office Stationery"
                android:gravity="center"
                android:id="@+id/textView5"
                android:layout_below="@+id/officetombol"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignRight="@+id/officetombol"
                android:layout_alignEnd="@+id/officetombol" />

            <TextView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:text="Fashion"
                android:gravity="center"
                android:id="@+id/textView4"
                android:layout_below="@+id/fashiontombol"
                android:layout_alignRight="@+id/fashiontombol"
                android:layout_alignEnd="@+id/fashiontombol"
                android:layout_alignLeft="@+id/fashiontombol"
                android:layout_alignStart="@+id/fashiontombol" />

            <TextView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:text="Mainan dan Hobi"
                android:gravity="center"
                android:id="@+id/textView3"
                android:layout_below="@+id/bolatombol"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_alignLeft="@+id/bolatombol"
                android:layout_alignStart="@+id/bolatombol" />

            <TextView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:text="Dapur"
                android:gravity="center"
                android:id="@+id/textView6"
                android:layout_below="@+id/dapurtombol"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignRight="@+id/dapurtombol"
                android:layout_alignEnd="@+id/dapurtombol" />

            <TextView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:text="Other"
                android:gravity="center"
                android:id="@+id/textView7"
                android:layout_below="@+id/othertombol"
                android:layout_alignRight="@+id/othertombol"
                android:layout_alignEnd="@+id/othertombol"
                android:layout_alignLeft="@+id/othertombol"
                android:layout_alignStart="@+id/othertombol" />


        </RelativeLayout>
        </ScrollView>
            </RelativeLayout>

    </RelativeLayout>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:src="@drawable/logoberanda"

            android:layout_centerHorizontal="true"
            />

        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/pengaturan"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"/>

        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:src="@drawable/beranda"
            android:id="@+id/berandatombol" />

        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/akunsaya"
            android:id="@+id/akuntombol"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            />

        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/pencarian"
            android:layout_alignTop="@+id/favtombol"
            android:layout_centerInParent="true"
            android:id="@+id/pencariantombol" />

        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/favorite"
            android:id="@+id/favtombol"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@+id/berandatombol"
            android:layout_toEndOf="@+id/berandatombol"
            android:layout_toStartOf="@id/pencariantombol"
            android:layout_toLeftOf="@id/pencariantombol"/>
        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/berita"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@+id/pencariantombol"
            android:layout_toEndOf="@+id/pencariantombol"
            android:layout_toStartOf="@id/akuntombol"
            android:layout_toLeftOf="@id/akuntombol" />

</RelativeLayout>
